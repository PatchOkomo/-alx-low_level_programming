#include "main.h"
/**
 * _strlen - returns the length of a string
 * @s: string
 * Return: length
 */

int _strlen(char *s)
{
	int length = 0;

	while (*s != '\0')
	{
		length++;
		s++;
	}

	return (length);
}

/**
 * _atoi - converts a string to an integer
 * @s: String to be converted to an integer
 * Return: 0 if n integers found, integer otherwise
